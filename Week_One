# List of students
student_list = [113, 175, 12]

# Students per group
students_per_group = 24

# create list
Full_groups = []
leftover_groups = []

print("list one:",student_list[0])
print("list two:",student_list[1])
print("list three:",student_list[2])

print("list one will have:",student_list[0]//students_per_group,"groups")
print("list two will have:",student_list[1]//students_per_group,"groups")
print("list three will have:",student_list[2]//students_per_group,"groups")

print(student_list[0]%students_per_group)

